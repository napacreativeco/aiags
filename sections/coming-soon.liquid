        <div class="lower">
            <div class="icon">
                <img src={{ 'AIAGSemblem.png' | asset_img_url: 'large' }} alt="All Is A Gentle Spring" />
            </div>
            <div>
            {{ section.settings.bodytext }}
            {%- form 'storefront_password', class: 'password-form' -%}
                <div class="password-field field{% if form.errors %} password-field--error{% endif %}">
                <input
                    type="password"
                    name="password"
                    id="Password"
                    class="field__input"
                    autocomplete="current-password"
                    {% if form.errors %}
                    aria-invalid="true"
                    aria-describedby="PasswordLoginForm-password-error"
                    {%- endif -%}
                    placeholder="{{ 'general.password_page.login_form_password_placeholder' | t }}"
                >
                <label class="field__label" for="{{ 'general.password_page.login_form_password_label' | t }}">{{ 'general.password_page.login_form_password_placeholder' | t }}</label>
                {%- if form.errors -%}
                    <div id="PasswordLoginForm-password-error" role="status">
                    <span class="visually-hidden">{{ 'accessibility.error' | t }}</span>
                    <span class="form__message">{% render 'icon-error' %} {{ 'general.password_page.login_form_error' | t }}</span>
                    </div>
                {%- endif -%}
                </div>
                <button name="commit" class="password-button button button--outline">
                {{ 'general.password_page.login_form_submit' | t }}
                </button>
            {%- endform -%}
            </div>

        </div>
    </div>
</div>



{%comment%}
*
* Settings
*
{%endcomment%}
{% schema %}
{
    "name": "Coming Soon",
    "tag": "section",
    "settings": [
        {
            "type": "text",
            "id": "headline",
            "label": "Headline",
            "default": "Good things come to those who wait"
        },
        {
            "type": "textarea",
            "id": "bodytext",
            "label": "Body text",
            "default": "we're away writing our next chapter. all is a gentle spring will return soon. a curated selection of our pieces can be purchased via our friends at apoc and café forgot all existing orders shall be dispatched by our team. for queries about orders placed before june seventh, simply contact info@gentlespring.co"
        }
    ]
}
{% endschema %}

{%comment%}
*
* CSS
*
{%endcomment%}
<style>
    body {
        background: #000;
        height: 100%;
        width: 100vw;
        color: #fff;
        overflow-x: hidden;
    }
    main {
        margin: 0;
    }
    .coming-soon {
        padding: 40px;
    }
    .coming-soon .row {
        height: 100vh;
        max-height: calc(100vh - 80px);
        width: 100%;
        display: flex;
        flex-direction: column;
        justify-content: space-between;
    }
    .coming-soon svg {
        width: 60vw;
    }
    .coming-soon .lower {
        display: grid;
        grid-template-columns: 1fr 1fr;
        font-size: 1.4rem;
    }

    .coming-soon .lower img {
        filter: invert(1);
        width: 32px;
        height: 50px;
    }
    .coming-soon .lower .icon {
            display: flex;
            align-self: flex-end;
    }


    @media screen and (max-width: 767px) {
        .coming-soon svg {
            width: 100%;
        }
        .coming-soon .lower {
            display: grid;
            grid-template-columns: 1fr;
            grid-template-rows: 1fr 1fr;
            grid-gap: 60px;
            font-size: 1.4rem;
        }

    }
</style>